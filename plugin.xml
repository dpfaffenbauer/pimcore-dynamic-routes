<?xml version="1.0"?><zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>
        <pluginName>DynamicRoutes</pluginName>
        <pluginDescription><![CDATA[
			Set up dynamic routes, routes based on documents or controllers and actions not static urls.
		]]></pluginDescription>
        <pluginNiceName>Dynamic Routes</pluginNiceName>
        <pluginVersion>0.1</pluginVersion>
        <pluginRevision>1</pluginRevision>
        <pluginBuildTimestamp></pluginBuildTimestamp>
        <pluginClassName>DynamicRoutes_Plugin</pluginClassName>
        <pluginIncludePaths>
            <path>/DynamicRoutes/controllers</path>
            <path>/DynamicRoutes/models</path>
        </pluginIncludePaths>
        <pluginNamespaces>
            <namespace>DynamicRoutes</namespace>
        </pluginNamespaces>
        <pluginJsPaths>
            <path>/DynamicRoutes/static/js/admin.js</path>
        </pluginJsPaths>
    </plugin>
</zend-config>